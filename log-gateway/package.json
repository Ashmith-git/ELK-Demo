{
  "name": "log-gateway",
  "version": "1.0.0",
  "description": "A gateway service for the ELK logs",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "npm run clean && npm run buildTsc",
    "buildWatch": "npm run clean && npm run buildWatchTsc",
    "buildTsc": "./node_modules/.bin/tsc",
    "buildWatchTsc": "./node_modules/.bin/tsc -w",
    "clean": "./node_modules/.bin/rimraf ./dist",
    "test": "./node_modules/bin/mocha -b -c dist/**/*.test.js",
    "testWatch": "./node_modules/bin/mocha -b -c -w dist/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mobiwize/docker-elk.git"
  },
  "keywords": [
    "logs",
    "logging"
  ],
  "author": "Slava Shpitalny",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mobiwize/docker-elk/issues"
  },
  "homepage": "https://github.com/mobiwize/docker-elk#readme",
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/request": "^2.0.0",
    "rimraf": "2.6.1",
    "typescript": "2.4.2"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "request": "^2.81.0"
  }
}
